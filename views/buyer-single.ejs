<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">
  <title>Agenzia Immobiliare</title>
  <meta content="" name="description">
  <meta content="" name="keywords">
  <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">
  <link href="/public/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/public/assets/vendor/ionicons/css/ionicons.min.css" rel="stylesheet">
  <link href="/public/assets/vendor/animate.css/animate.min.css" rel="stylesheet">
  <link href="/public/assets/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet">
  <link href="/public/assets/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">
  <link href="/public/assets/css/style.css" rel="stylesheet">
</head>
<body>
  <div class="click-closed"></div>
  <!-- ======= Navbar ======= -->
  <%- include('navbar.ejs') %>
  <!-- End Navbar -->
  <main id="main">
    <!-- ======= Intro Section ======= -->
    <section class="intro-single">
      <div class="container">
        <div class="row">
		<%  var parsed_acquirente = JSON.parse(acquirente) %>
          <div class="col-md-12 col-lg-8">
            <div class="title-single-box">
              <h1 class="title-single"> <%= parsed_acquirente[0][0].nome %> <%= parsed_acquirente[0][0].cognome %> </h1>
            </div>
          </div>
          <div class="col-md-12 col-lg-4">
            <nav aria-label="breadcrumb" class="breadcrumb-box d-flex justify-content-lg-end">
              <ol class="breadcrumb">
                <li class="breadcrumb-item">
                  <a href="/home">Home</a>
                </li>
                <li class="breadcrumb-item">
                  Acquirenti
                </li>
				<li class="breadcrumb-item active" aria-current="page">
                  <%= parsed_acquirente[0][0].nome %> <%= parsed_acquirente[0][0].cognome %>
                </li>
              </ol>
            </nav>
          </div>
        </div>
      </div>
    </section>
	<!-- End Intro Section -->
    <!-- ======= Buyer Section ======= -->
    <section class="agent-single">
      <div class="container">
        <div class="row">
          <div class="col-sm-12">
            <div class="row">
              <div class="col-md-5 section-md-t3">
                <div class="agent-info-box">
                  <div class="agent-content mb-3">
                    <div class="info-agents color-a">
                      <p>
                        <strong>Telefono: </strong>
                        <span class="color-text-a">  <%= parsed_acquirente[0][0].telefono %> </span>
                      </p>					
                      <p>
                        <strong>Email: </strong>
                        <span class="color-text-a"> <%= parsed_acquirente[0][0].email %> </span>
                     </p>                   
                   </div>
                  </div>
                 </div>
              </div>
            </div>
          </div>
          <div class="col-md-12 section-tacquirente">
            <div class="title-box-d">
              <h3 class="title-d">Contratti dell' acquirente</h3>
            </div>
          </div>      
        </div>
      </div>
    </section><!-- End Buyer Section -->
    <!-- =======  Buyer's Contract Section ======= -->
<section class="agents-grid grid">
      <div class="container">
        <div class="row">
		  <% for(var i=0; i <  parsed_acquirente[1].length; i++) { %>
          <div class="col-md-4">
		  <div class="card-margin">
              <div class="card-overlay card-overlay-hover">
                <div class="card-header-d">
                  <div class="card-title-d align-self-center">
                    <h3 class="title-d">
                      <a href="/proprieta/<%= parsed_acquirente[1][i].id_proprieta %>" class="link-two"><%= parsed_acquirente[1][i].via %></a>
                    </h3>
                  </div>
                </div>
                <div class="card-body-d">               
                  <div class="info-agents color-a">
                    <p>
						Data Inizio: <%= parsed_acquirente[1][i].inizio %></strong> 
                    </p>
					<p>
						Data Fine: <%= parsed_acquirente[1][i].fine %></strong> 
                    </p>
					<p>
						ID Contratto: <%= parsed_acquirente[1][i].id %></strong> 
                    </p>
                  </div>
                </div>             
              </div>
          </div>         
        </div>
		<% } %>
		<% for(var i=0; i <  parsed_acquirente[2].length; i++) { %>
          <div class="col-md-4">
		  <div class="card-margin">
              <div class="card-overlay card-overlay-hover">
                <div class="card-header-d">
                  <div class="card-title-d align-self-center">
                    <h3 class="title-d">
                      <a href="/proprieta/<%= parsed_acquirente[2][i].id_proprieta %>" class="link-two"><%= parsed_acquirente[2][i].via %></a>
                    </h3>
                  </div>
                </div>
                <div class="card-body-d">               
                  <div class="info-agents color-a">
                    <p>
						Prezzo: <%= parsed_acquirente[2][i].prezzo %></strong> 
                    </p>
					<p>
						ID Contratto: <%= parsed_acquirente[2][i].id %></strong> 
                    </p>
                  </div>
                </div>             
              </div>
          </div>         
        </div>
		<% } %>
		</div>
	</div>
	</section>
  </main>
  <!-- ======= Footer ======= -->
  <%- include('footer.ejs') %>
  <!-- End  Footer -->
  <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>
  <div id="preloader"></div>
  <!--  JS Files -->
  <script src="/public/assets/vendor/jquery/jquery.min.js"></script>
  <script src="/public/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/public/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
  <script src="/public/assets/vendor/php-email-form/validate.js"></script>
  <script src="/public/assets/vendor/owl.carousel/owl.carousel.min.js"></script>
  <script src="/public/assets/vendor/scrollreveal/scrollreveal.min.js"></script>
  <script src="/public/assets/js/main.js"></script>
</body>
</html>