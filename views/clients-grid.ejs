<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">
  <title>Agenzia Immobiliare - <%= tipo %></title>
  <meta content="" name="description">
  <meta content="" name="keywords">
  <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">
  <link href="/public/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/public/assets/vendor/ionicons/css/ionicons.min.css" rel="stylesheet">
  <link href="/public/assets/vendor/animate.css/animate.min.css" rel="stylesheet">
  <link href="/public/assets/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet">
  <link href="/public/assets/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">
  <link href="/public/assets/css/style.css" rel="stylesheet">
</head>
<body>
  <div class="click-closed"></div>
  <!-- ======= Navbar ======= -->
  <%- include('navbar.ejs') %>
  <!-- End Navbar -->
  <main id="main">
    <!-- =======Intro Section ======= -->
    <section class="intro-single">
      <div class="container">
        <div class="row">
          <div class="col-md-12 col-lg-8">
            <div class="title-single-box">
              <h1 class="title-single"><%= tipo %> <a href="/inseriscicliente"><button style="float:right; background-color:#26c2e9; color:white; border: 1px solid white;">+</button></a></h1>
            </div>
          </div>
          <div class="col-md-12 col-lg-4">
            <nav aria-label="breadcrumb" class="breadcrumb-box d-flex justify-content-lg-end">
              <ol class="breadcrumb">
                <li class="breadcrumb-item">
                  <a href="/home">Home</a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">
                  <%= tipo %>
                </li>
              </ol>
            </nav>
          </div>
        </div>
      </div>
    </section><!-- End Intro Section-->
    <!-- ======= Clients Section ======= -->
    <section class="grid">
      <div class="container">
        <div class="row">
		<%  var parsed_clienti = JSON.parse(clienti) %>
		  <% for(var i=0; i <  parsed_clienti[0].length; i++) { %>
          <div class="col-md-4">
		  <div class="card-margin">
              <div class="card-overlay card-overlay-hover">
                <div class="card-header-d">
                  <div class="card-title-d align-self-center">
                    <h3 class="title-d">
                      <a href="/acquirenti/<%= parsed_clienti[0][i].cf %>" class="link-two"><%= parsed_clienti[0][i].nome %>
                        <br> <%= parsed_clienti[0][i].cognome %></a>
                    </h3>
                  </div>
                </div>
                <div class="card-body-d">
                 
                  <div class="color-a">
                    <p>Telefono: </strong> <%= parsed_clienti[0][i].telefono %>
                    </p>
                    <p>
                      <strong>Email: </strong> <%= parsed_clienti[0][i].email %>
                    </p>
                  </div>
                </div>  
              </div>
          </div>         
        </div>
		<% } %>
		<% for(var i=0; i <  parsed_clienti[1].length; i++) { %>
          <div class="col-md-4">
		  <div class="card-margin">
              <div class="card-overlay card-overlay-hover">
                <div class="card-header-d">
                  <div class="card-title-d align-self-center">
                    <h3 class="title-d">
                      <a href="/proprietari/<%= parsed_clienti[1][i].cf %>" class="link-two"><%= parsed_clienti[1][i].nome %>
                        <br> <%= parsed_clienti[1][i].cognome %></a>
                    </h3>
                  </div>
                </div>
                <div class="card-body-d">
                 
                  <div class="color-a">
                    <p>Telefono: </strong> <%= parsed_clienti[1][i].telefono %>
                    </p>
                    <p>
                      <strong>Email: </strong> <%= parsed_clienti[1][i].email %>
                    </p>
                  </div>
                </div>  
              </div>
          </div>         
        </div>
		<% } %>
		</div>      
      </div>
    </section><!-- End Clients Section-->
  </main>
  <!-- ======= Footer ======= -->
  <%- include('footer.ejs') %>
  <!-- End  Footer -->
  <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>
  <div id="preloader"></div>
  <!-- JS Files -->
  <script src="/public/assets/vendor/jquery/jquery.min.js"></script>
  <script src="/public/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/public/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
  <script src="/public/assets/vendor/php-email-form/validate.js"></script>
  <script src="/public/assets/vendor/owl.carousel/owl.carousel.min.js"></script>
  <script src="/public/assets/vendor/scrollreveal/scrollreveal.min.js"></script>
  <script src="/public/assets/js/main.js"></script>
</body>

</html>