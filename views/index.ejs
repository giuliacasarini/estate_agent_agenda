                <a href="/agenti">Tutti gli Agenti
                  <span class="ion-ios-arrow-forward"></span>
                </a>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
		<%  var parsed_agente = JSON.parse(agente) %>
		  <% for(var i=0; i <  parsed_agente.length; i++) { %>
          <div class="col-md-6">
            <div class="card-box-d">
              <div class="card-img-d">
                <img src="/public/assets/img/agente.jpg" alt="" class="img-d img-fluid">
              </div>
              <div class="card-overlay card-overlay-hover">
                <div class="card-header-d">
                  <div class="card-title-d align-self-center">
                    <h3 class="title-d">
                      <%= parsed_agente[i].nome %> 
                        <br> <%= parsed_agente[i].cognome %>
                    </h3>
                  </div>
                </div>
                <div class="card-body-d">
                  <div class="info-agents color-a">
                    <p>
                      <strong>Telefono: </strong> <%= parsed_agente[i].telefono %> 
                    </p>
                    <p>
                      <strong>Email: </strong> <%= parsed_agente[i].email %> 
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
		  <% } %>
        </div>
      </div>
    </section>
